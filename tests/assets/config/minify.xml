<?xml version="1.0"?>
<config name="Minify">
    <description>Minify code</description>
    <pathFrom>src</pathFrom>
    <pathTo>deploy</pathTo>
    <handlers>
        <handler>space</handler>
        <handler>tabulation</handler>
        <handler>break</handler>
    </handlers>
    <extensions>
        <ext>php</ext>
        <ext>js</ext>
        <ext>css</ext>
    </extensions>
</config>